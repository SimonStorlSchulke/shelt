{
  "kind": "collectionType",
  "collectionName": "animals",
  "info": {
    "singularName": "animal",
    "pluralName": "animals",
    "displayName": "Animal",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Gender": {
      "type": "enumeration",
      "enum": [
        "female",
        "male",
        "other"
      ]
    },
    "Castrated": {
      "type": "boolean"
    },
    "Thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Shoulderheight": {
      "type": "integer",
      "required": true,
      "default": 35
    },
    "Description": {
      "type": "richtext"
    },
    "CustumArticleLink": {
      "type": "string"
    },
    "BirthDate": {
      "type": "date"
    },
    "Species": {
      "type": "string"
    },
    "Race": {
      "type": "string"
    },
    "Tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "Animals"
    },
    "article": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::article.article",
      "inversedBy": "Animals"
    }
  }
}
